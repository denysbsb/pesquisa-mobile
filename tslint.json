{
  "rulesDirectory": [
    "node_modules/codelyzer"
  ],
  "rules": {
    "class-name": false,
    "comment-format": {"options": ["check-space"]},
    "eofline": false,
    "indent": {"options": ["spaces"]},
    "no-debugger": false,
    "no-duplicate-variable": false,
    "no-eval": false,
    "no-internal-module": false,
    "no-trailing-whitespace": false,
    "no-var-keyword": false,
    "one-line": {
      "options": []
    },
    "quotemark": {"options": ["single"]},
    "radix": false,
    "semicolon": {"options": ["always"]},
    "triple-equals": {"options": ["allow-null-check"]},
    "typedef-whitespace": {
      "options": {}
    },
    "variable-name": {"options": ["ban-keywords"]},
    "whitespace": {
      "options": [ ]
    },
    "no-consecutive-blank-lines": false,
    "import-destructuring-spacing": false,
    "invoke-injectable": false,
    "templates-use-public": false,
    "use-pipe-transform-interface": false
  }
}
